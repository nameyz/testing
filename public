
<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<html>
<head>
    <title>阶乘</title>
</head>
<body>
<h1>1到10的阶乘为：</h1>
<%
  for(int i=1;i<=10;i++){
      int x=i;
      out.print(x+"!=");
      for(int j=i-1;j>=1;j--){
         x=x*j;
      }
      out.print(x);
      %>
<br/>
<%
  }
%>
</body>
</html>
